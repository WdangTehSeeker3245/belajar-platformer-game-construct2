<?xml version="1.0" encoding="utf-8" ?>
<c2uistate>
    <!--This is a UI state file - its content describes the user interface settings when the project was last open.
It is entirely optional and the project will load without it.  If you are placing the project under source control,
you probably do not want to add any .uistate.xml files to the repository.-->
    <configurations>
        <active>
            <configuration name="HTML5" />
        </active>
        <display name="HTML5" />
    </configurations>
    <open-tabs>
        <layout order="1">Start Menu</layout>
        <layout active="1" order="3">Gameplay</layout>
        <layout order="5">Gameover</layout>
        <event-sheet order="2">Start Menu Event</event-sheet>
        <event-sheet order="4">Gameplay Event</event-sheet>
        <event-sheet order="6">Gameover Event</event-sheet>
    </open-tabs>
    <properties>
        <property expanded="1" name="About" />
        <property expanded="1" name="Project settings" />
        <property expanded="1" name="Window Size" />
        <property expanded="1" name="Configuration Settings" />
    </properties>
    <families-expanded root="1">
        <family expanded="0" name="PauseMenu" />
    </families-expanded>
    <folders-expanded>
        <folder expanded="1" path="object-folder/" />
        <folder expanded="1" path="layout-folder/" />
        <folder expanded="1" path="event-folder/" />
        <folder expanded="1" path="file-folder/" />
        <folder expanded="1" path="sound-folder/" />
        <folder expanded="1" path="music-folder/" />
        <folder expanded="0" path="file-folder/Icons/" />
        <folder expanded="1" path="animation-folder/start_button/" />
        <folder expanded="1" path="animation-folder/load_button/" />
        <folder expanded="1" path="animation-folder/exit_button/" />
        <folder expanded="1" path="animation-folder/PlayerSprite/" />
        <folder expanded="1" path="animation-folder/CollisionGround/" />
        <folder expanded="1" path="animation-folder/GameTitle/" />
        <folder expanded="1" path="animation-folder/CollisionGround2/" />
        <folder expanded="1" path="animation-folder/CollisionGround3/" />
        <folder expanded="1" path="animation-folder/DeadCollision/" />
        <folder expanded="1" path="animation-folder/GameoverText/" />
        <folder expanded="1" path="animation-folder/playagain_button/" />
        <folder expanded="1" path="animation-folder/startmenu_button/" />
        <folder expanded="1" path="animation-folder/PausePanel/" />
        <folder expanded="1" path="animation-folder/PauseText/" />
    </folders-expanded>
</c2uistate>
